{
  "forwards": {
    "my-api-service": {
      "type": "kubernetes",
      "group": "dev",
      "localPort": 8080,
      "enabled": true,
      "context": "dev-cluster",
      "namespace": "default",
      "service": "api-service",
      "servicePort": 80
    },
    "database-proxy": {
      "type": "socket",
      "group": "dev",
      "localPort": 5432,
      "enabled": true,
      "remoteHost": "db.example.com",
      "remotePort": 5432
    },
    "monitoring-service": {
      "type": "kubernetes",
      "group": "monitoring",
      "localPort": 9090,
      "enabled": false,
      "context": "prod-cluster",
      "namespace": "monitoring",
      "service": "prometheus",
      "servicePort": 9090
    }
  }
}
